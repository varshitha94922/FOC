1.Calculator


#include<stdio.h>
int main()
{
	int a,b,n,sum=0;
	printf("enter 1st num: ");
	scanf("%d",&a);
	printf("enter 2nd num: ");
	scanf("%d",&b);
	printf("1.Addition \n2.Subtraction \n3.Multiplication \n4.Division");
	printf("\nenter your choice: ");
	scanf("%d",&n);
	if(n==1)
	{
		sum=a+b;
		printf("Adiition is: %d",sum);
	}
	else if(n==2)
	{
		sum=a-b;
		printf("subtraction is: %d",sum);
	}
	else if(n==3)
	{
		sum=a*b;
		printf("multiplicatin is: %d",sum);
	}
	else if(n==4)
	{
		sum=a/b;
		printf("division is: %d",sum);
	}
}

2.Generate a number series

#include<stdio.h>
#include<conio.h>
int main()
{
    int N, i;
    printf("Enter the value of N (limit): ");
    scanf("%d", &N);
    printf("\n");
    for(i=1; i<=N; i++)
    {
        if(i==N)
            printf("%d", i);
        else
            printf("%d,", i);
    }
    getch();
    return 0;
}

3.Reverse a number and array


#include <stdio.h>
#define N 1000

int main() {
    int arr[N];

    int n;
    printf("Enter the size of the array: ");
    scanf("%d", &n);
    printf("Enter an array: ");
    for (int i = 0; i< n; i++){
        scanf("%d", &arr[i]);
    }
    printf("Reversed array: ");
    for (int i = n-1; i>=0; i--){
        printf("%d ", arr[i]);
    }

    return 0;
}

4.Decimal to binary


 #include <stdio.h>
int main() 
{
    int a[10], number, i, j;
    printf("\n Please Enter the Number You want to Convert  :  ");
    scanf("%d", &number);

    for(i = 0; number > 0; i++)
    {
        a[i] = number % 2;
        number = number / 2;
    }

    printf("\n Binary Number of a Given Number =  ");
    for(j = i - 1; j >= 0; j--)  {
        printf(" %d ", a[j]);
    }
    printf("\n");
    return 0;
}

5.Square and cube of a decimal


#include <stdio.h>
int main(){
    int num, square, cube;
    printf("Enter an integer: ");
    scanf("%d", &num);
    square = num * num;
    cube = num * num * num;
    printf("Square of %d is: %d \n", num, square);
    printf("Cube of %d is: %d", num, cube);

    return 0;
}

